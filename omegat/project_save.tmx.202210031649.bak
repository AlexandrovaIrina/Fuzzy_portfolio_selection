    <tu>
      <tuv lang="EN-US">
        <seg>1  Introduction</seg>
      </tuv>
      <tuv lang="RU-RU" changeid="ALEX-K" changedate="20221003T130812Z" creationid="ALEX-K" creationdate="20221003T130812Z">
        <seg>1  Введение</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="EN-US">
        <seg>2&lt;t0/&gt;\</seg>
      </tuv>
